}

.item_recommend .info_Subtitle {
  padding-left: 20rpx;
  margin-top: 12rpx;
  height: 34rpx;
  font-size: 26rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  display: flex;
  align-items: center;
}

.item_recommend .info_btn {
  width: 120rpx;
  height: 40rpx;
  border-radius: 21rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-left: 20rpx;
  margin-top: 22rpx;
}

.item_recommend .info_btn image {
  width: 7rpx;
