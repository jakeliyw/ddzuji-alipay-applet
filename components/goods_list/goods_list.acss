.goods_list {
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  padding: 0rpx 18rpx;
}

.goods_list .goods_item {
  width: 347rpx;
  /* height: 545rpx; */
  border-radius: 20rpx;
  background-color: #FFFFFF;
  margin-bottom: 20rpx;
  overflow: hidden;
  padding: 20rpx;
}
.characteristic_label {
  height: 35rpx;
  background-color: #E7F1FF;
  border-radius: 4rpx;
  display: inline-block;
  padding: 0rpx 8rpx;
  margin-right: 10rpx;
  margin-top: 10rpx;
}

.characteristic_label .text {
  font-size: 20rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #1777FF;
  line-height: 35rpx;
}
/* 首月1元icon */
.goods_list .goods_item .sale-icon {
  width: 109rpx;
  height: 40rpx;
  margin-left: 20rpx;
}

/* 商品图片  start*/

.goods_item .item_image image {
  background-size: contain !important;
  background-position: center center;
  max-height: 335rpx;
  max-width: 335rpx;
  border-radius: 20rpx;
}

.goods_item .item_image {
  width: 335rpx;
  height: 335rpx;
  border-radius: 20rpx;
  background-color: #FFFFFF;
}

.item_goods .item_image {
  position: relative;
}

.recently {
  height: 38rpx;
  width: 120rpx;
  background: linear-gradient(90deg, rgba(255, 84, 130, 1) 0%, rgba(255, 183, 183, 1) 100%);
  border-radius: 0rpx 19rpx 19rpx 0rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  position: absolute;
  top: 30rpx;
}

.recently .text {
  height: 22rpx;
  font-size: 22rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
}

/* 商品图片  end*/

/* 标签、地址 start*/

.goods_item .item_label {
  margin-top: 15rpx;
  margin-bottom: 10rpx;
  /* display: flex;
  flex-direction: row; */
}

.goods_item .item_label .item_isNew {
  display: inline-block;
  width: 64rpx;
  height: 33rpx;
  background: linear-gradient(90deg, rgba(255, 129, 85, 1) 0%, rgba(255, 72, 77, 1) 100%);
  border-radius: 6rpx;
  margin-right: 10rpx;
  font-size: 20rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 800;
  color: rgba(255, 255, 255, 1);
}

.goods_item .item_label .item_noNew {
  display: inline-block;
  width: 64rpx;
  height: 33rpx;
  background: #FFEC88;
  border-radius: 6rpx;
  margin-right: 10rpx;
  font-size: 20rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 800;
  color: #593A01;
}

.goods_item .item_label .item_isNew .text, .goods_item .item_label .item_noNew .text {
  font-size: 20rpx;
  font-family: PingFang SC-Heavy, PingFang SC;
  font-weight: 800;
  line-height: 33rpx;
  text-align: center;
}

.goods_item .item_address {
  display: inline-block;
}

.goods_item .item_address .address_text {
  height: 28rpx;
  display: flex;
  align-items: center;
}

.goods_item .item_address .address_text image {
  width: 15rpx;
  height: 20rpx;
  margin-left: 13rpx;
  margin-right: 5rpx;
}

.goods_item .item_address .address_text .text { 
  height: 24rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #999999;
}

/* 标签、地址 end */

/* 商品名 start */

.item_goodsName {
  width: 295rpx;
  font-size: 26rpx;
  font-family: PingFang SC-Bold, PingFang SC;
  font-weight: bold;
  color: #2E3139;
  line-height: 34rpx;
  overflow: hidden;
}

/* 商品名 end */

/* 价格、起租天数 start */

.item_price {
  display: flex;
  justify-content: space-between;
  align-items: flex-end;
  margin-top: 10rpx;
  /* margin-bottom: 10rpx; */
}

.item_price .price {
  color: #FF584F;
  margin-top:0rpx;
}

.item_price .price .price_unit {
  font-family: PingFang SC-Bold, PingFang SC;
  font-weight: bold;
  color: #F43139;
  font-size: 20rpx;
}

.item_price .price .price_text {
  font-family: PingFang SC-Bold, PingFang SC;
  font-weight: bold;
  color: #F43139;
  font-size: 20rpx;
}

.item_price .price .price_text .big {
  font-size: 34rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: bold;
  font-family: 'DINPro';
}

.item_price .item_day {
  font-size: 18rpx;
  font-family: PingFang SC-Regular, PingFang SC;
  font-weight: 400;
  color: #D2D2D2;
  display: inline-block;
  text-decoration:  line-through;
}

/* 价格、起租天数 end */

