.recommend_info {
  display: flex;
  align-items: flex-start;
}

.goods_recommend {
  background: linear-gradient(109deg, rgba(255, 255, 255, 1) 0%, rgba(244, 241, 255, 1) 100%);
}

.recommend_info .info_label {
  height: 34rpx;
  border-radius: 17rpx;
  padding: 0rpx 9rpx;
