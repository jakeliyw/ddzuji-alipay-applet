/* 弹出面板 */

.cover {
  position: fixed;
  left: 0;
  bottom: 0;
  z-index: 9;
  width: 100%;
  height: 100%;
}

.cover .shadow {
  position: absolute;
  left: 0;
  top: 0;
  z-index: 10;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, .5);
  opacity: 2;
}

.enjoyCover .shadow {
  position: absolute;
  left: 0;
  top: 0;
  z-index: 10;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, .4);
  opacity: 1;
}

.cover .panel {
  position: absolute;
  display: flex;
  flex-direction: column-reverse;
  /* padding-bottom: 108rpx; */
  left: 0;
  bottom: 0;
  z-index: 11;
  width: 100%;
  transform: translateY(100%);
  transform: translateZ(100);
}

.cover .attr_p {
  height: calc(100% - 100rpx);
}

.cover .layer {
  position: absolute;
  left: 0;
  top: 0;
  z-index: 11;
  width: 100%;
  height: 100%;
  transform: scale(1) !important;
  transform: translateZ(100);
  opacity: 1;
}

