}

.shunfeng {
  padding-left: 50rpx;
  height: 88rpx;
}

.express_view .message_view {
  display: inline-block;
  width: 0rpx;
  height: 72rpx;
  overflow: hidden;
  margin-left: 15rpx;
}

.express_view .message_view1 {
  display: inline-block;
  width: 40rpx;
  height: 72rpx;
  margin-left: -30rpx;
}

.express_view .form-row-content .message_view1 image {
  width: 40rpx;
    height: 72rpx;
  display: inline-block;
}

.express_view .form-row-content .message_view image {
  width: 226rpx;
  height: 72rpx;
  float: left;
}


.hidemes .message_view {
  transition: 1s;
  width: 0rpx !important;
}

.showms .message_view {
  transition: 1s;
  width: 226rpx !important;
}

.hidemes  .right_hidemes{
   transition: 0.5s;
   margin-left: 0rpx !important;
  
}

.showms  .right_hidemes{
   transition: 0.5s;
   margin-left: 0rpx !important;
   
}
 