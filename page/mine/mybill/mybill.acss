/* 底部 */

.bottom {
  position: fixed;
  left: 0;
  bottom: 0;
  z-index: 3;
  width: 100%;
  height: 101rpx;
  background-color: #fff;
}

.bottom .line {
  position: absolute;
  left: 0;
  top: 0;
  z-index: 4;
  width: 200%;
  height: 1px;
  background-color: #ddd;
  transform: scale(.5);
  margin-left: -50%;
}

.bottom .wrapper {
  display: flex;
  flex-flow: row nowrap;
  justify-content: space-between;
  align-items: center;
  width: 100%;
  height: 100%;
  padding: 0 30rpx 0;
  box-sizing: border-box;
}

.bottom .wrapper button {
  border: 0;
  outline: 0;
  width: 160rpx;
  height: 48rpx;
