  position: absolute;
  margin-left: -26rpx;
  margin-top: 3rpx;
}

.rend_insurance {
  width: 100%;
  height: 90rpx;
  padding: 30rpx 35rpx;
  background-color: white;
  font-size: 28rpx;
  color: #999999;
  margin-top: 20rpx;
}

.rend_insurance_content {
  display: flex;
