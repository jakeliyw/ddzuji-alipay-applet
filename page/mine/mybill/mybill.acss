}

.order-item .goods-des .goods-img .pic {
  width: 160rpx;
  height: 160rpx;
}

.order-item .goods-des .content {
  width: 74%;
  height: 100%;
  padding-top: 36rpx;
  box-sizing: border-box;
}

.order-item .content .goods-name {
  width: 100%;
  height: 28rpx;
  font-size: 28rpx;
  color: rgb(51, 51, 51);
  line-height: 1;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin-bottom: 20rpx;
  box-sizing: border-box;
}

.order-item .content .other-pay {
  width: 100%;
  height: 24rpx;
  font-size: 24rpx;
  color: #999;
  line-height: 1;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin-bottom: 16rpx;
}

.order-item .content .total-price {
  width: 100%;
  height: 28rpx;
  font-size: 28rpx;
  color: #333;
  line-height: 1;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin-bottom: 20rpx;
  box-sizing: border-box;
}

.order-item .content .zhima {
  width: 170rpx;
  height: 30rpx;
}

.cover {
  position: fixed;
  left: 0;
  bottom: 0;
  z-index: 9;
  width: 100%;
  height: 100%;
}

.cover .shadow {
  position: absolute;
  left: 0;
  top: 0;
  z-index: 10;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, .5);
  opacity: 0;
}

.enjoyCover .shadow {
  position: absolute;
  left: 0;
  top: 0;
  z-index: 10;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, .4);
  opacity: 0;
}

.cover .panel {
  position: absolute;
  left: 0;
  bottom: 0;
  z-index: 11;
  width: 100%;
  opacity: 1;
}

.cover .layer {
  position: absolute;
  left: 0;
  top: 0;
  z-index: 11;
  width: 100%;
  height: 100%;
  transform: scale(1) !important;
  transform: translateZ(100);
  opacity: 0;
}

/* 租金 */

.rend_pay {
  width: 100%;
  background-color: #fff;
  margin-bottom: 20rpx;
  box-sizing: border-box;
  margin-top: 10rpx;
  padding-bottom: 30rpx;
}

.rend_pay .rend_mon {
  position: relative;
  display: flex;
  flex-flow: row wrap;
  padding-top: 38rpx;
  box-sizing: border-box;
}

.rend_pay .rend_repay {
  flex: 1;
  max-width: 50%;
  min-width: 33.3%;
  text-align: center;
}

.rend_pay .rend_mon .rend_repay .rend_money {
  font-size: 32rpx;
  line-height: 1;
  color: #333;
}

.rend_pay .rend_mon .rend_repay .rend_state {
  font-size: 24rpx;
  line-height: 1;
  margin-top: 8rpx;
  color: #999;
}

.rend_pay .rend_mon .under-line {
  position: absolute;
  left: 0;
  top: 0;
  width: 1380rpx;
  height: 1px;
  background-color: #ddd;
  transform: scale(.5);
  margin-left: -315rpx;
  margin-top: 180rpx;
}

.rend_detail {
  position: relative;
  width: 100%;
  line-height: 30rpx;
  font-size: 24rpx;
  color: #999;
  padding: 0rpx 60rpx;
  line-height: 1;
  margin-top: 38rpx;
  box-sizing: border-box;
}

.rend_detail .rend_circle {
  position: absolute;
  left: 50rpx;
  top: 0;
  width: 20rpx;
  height: 20rpx;
  font-size: 20rpx;
  line-height: 24rpx;
  border: 1px solid #999;
  border-radius: 50%;
