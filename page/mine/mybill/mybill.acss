.order-item {
  width: 100%;
  height: 243rpx;
  background-color: #fff;
  padding: 0 30rpx;
  box-sizing: border-box;
}

.order-item .goods-des {
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
