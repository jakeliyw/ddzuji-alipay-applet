  font-size: 26rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #010101;
  margin-top: 27rpx;
  width: 100%;
  overflow: hidden;
}

.coupon_view .coupon_item .coupon_item_center .item_center_center {
  font-size: 22rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #999999;
  margin-top: 15rpx;
}

.coupon_view .coupon_item .coupon_item_center .item_center_bottom {
  font-size: 22rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #999999;
  margin-top: 20rpx;
  display: flex;
  align-items: center;
}

.coupon_view .coupon_item .coupon_item_center .item_center_bottom .text {
  height: 22rpx;
  display: inline-block;
}

.coupon_view .move_rule .coupon_item_center .item_center_bottom image {
  transform: scaleY(-1);
}

