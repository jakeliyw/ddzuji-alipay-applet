  width: 180rpx;
  height: 166rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.coupon_view .coupon_item .coupon_item_left .coupon_type1 {
  color: #FF584F;
  text-align: center;
  width: 100%;
}

