.receive_view {
  background: #FCF4E7 url("/2020081816504167997.png") no-repeat;
  background-size: 100%;
  min-height: 540rpx;
  width: 100%;
}

.coupon_view {
  padding: 10rpx 30rpx 170rpx 30rpx;
}

.coupon_view .coupon_explain {
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #999999;
  line-height: 36rpx;
  background: #FFFFFF;
  border-radius: 0px 0px 20rpx 20rpx;
  margin-top: -35rpx;
  padding: 65rpx 30rpx 30rpx 30rpx;
}

.coupon_view .coupon_item {
  /* background: url('/image/coupon_item.png') 0% 0% / 100% 100%; */
  background-color: #FFFFFF;
  border-radius: 20rpx;
  width: 100%;
  height: 196rpx;
  display: flex;
  justify-content: space-between;
  margin-top: 20rpx;
  position: relative;
  overflow: hidden;
  box-shadow:1rpx 1rpx 24rpx 0rpx rgba(252,244,231,1);
}

.coupon_view .move_rule {
  border-radius: 20rpx 20rpx 0px 0px;
  box-shadow: 0px 1rpx 24rpx 0px rgba(237, 237, 237, 1);
  /* background: url('/image/coupon_item1.png') 0% 0% / 100% 100%; */
}

