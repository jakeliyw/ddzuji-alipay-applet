}

.recomCoupon_item .item_title {
  width: 100%;
  font-size: 26rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  height: 37rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.recomCoupon_item .item_describe {
  height: 50rpx;
  font-size: 28rpx;
  color: #333333;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-top: 16rpx;
}

.recomCoupon_item .item_describe .text {
  font-family:PingFang SC;
  font-weight: 500;
