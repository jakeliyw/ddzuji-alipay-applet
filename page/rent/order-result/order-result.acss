/* 活动坑位 */

.activity-banner {
  width: 100%;
  height: 180rpx!important;
  margin-top: 42rpx;
  background-color: white;
}

.activity-banner swiper {
  width: 100%;
  height: 180rpx!important;
  background-color: #fff;
  border-radius: 20rpx;
}

.activity-banner .item {
  width: 690rpx;
  height: 180rpx;
  padding: 0rpx 4rpx;
  display: flex;
  justify-content: center;
}

.activity-banner .item view {
  width: 690rpx;
  height: 180rpx;
  border-radius: 12rpx;
}

.activity-banner swiper .item, .activity-banner swiper navigator {
  width: 100%;
  height: 100%!important;
}

.activity-banner swiper .item image {
  width: 690rpx;
  height: 180rpx;
  border-radius: 12rpx;
}

.activity-banner .a-swiper-indicator {
  bottom: -13rpx !important;
  right: 70rpx !important;
  width: auto !important;
}