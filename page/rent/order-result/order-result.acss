page {
  background-color: rgb(247, 247, 247);
  box-sizing: border-box;
}

.container {
  /* background: linear-gradient(270deg, rgba(255, 72, 77, 1) 0%, rgba(255, 129, 85, 1) 100%); */
  background-color: #FFFFFF;
  border-radius: 0px 0px 20rpx 20rpx;
  padding: 0rpx 30rpx;
  overflow: hidden;
  border-top: 1rpx solid #DDDDDD;
  padding-bottom: 100rpx;
}

.state-mess {
  display: flex;
  flex-direction: column;
  width: 100%;
  height: 400rpx;
  padding-top: 60rpx;
  box-sizing: border-box;
}

.state-mess .state-icon {
  display: flex;
  justify-content: center;
  width: 100%;
}

.state-mess .state-icon image {
  width: 120rpx;
  height: 120rpx;
}

.state-mess .title {
  display: flex;
  justify-content: center;
  width: 100%;
  margin-top: 30rpx;
  font-size: 34rpx;
  color: rgb(51, 51, 51);
  line-height: 1;
}

.state-mess .extra {
  display: flex;
  justify-content: center;
  width: 100%;
  height: 32rpx;
  margin-top: 20rpx;
  font-size: 30rpx;
  color: #888;
  line-height: 32rpx;
}

.state-mess .zhima-icon {
  display: flex;
  justify-content: center;
  width: 100%;
  margin-top: 20rpx;
}

.state-mess .zhima-icon image {
  width: 191rpx;
  height: 23rpx;
}

.pay-detail {
  display: flex;
  flex-direction: column;
  width: 100%;
  background-color: #fff;
  margin-top: 20rpx;
}

.pay-detail .pay-month {
  display: flex;
  flex-direction: column;
  width: 100%;
  padding: 40rpx 30rpx;
  box-sizing: border-box;
}

.pay-detail .pay-month .single-pay {
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  justify-content: space-between;
  width: 100%;
  font-size: 28rpx;
  color: rgb(51, 51, 51);
  line-height: 1;
  margin-bottom: 30rpx;
}

.pay-detail .pay-month .single-pay .cut {
  font-size: 25rpx;
}

.pay-detail .pay-month .single-pay .rmb {
  font-size: 20rpx;
}

.pay-detail .pay-month .month {
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  justify-content: space-between;
  width: 100%;
  font-size: 28rpx;
  color: #999;
  line-height: 1;
}

.pay-detail .pay-month .instruction {
  width: 100%;
  font-size: 24rpx;
  color: #999;
  line-height: 1;
  margin-top: 26rpx;
}

.pay-detail .divide-line {
  width: 100%;
  height: 0;
  border-top: 2rpx solid #ddd;
}

.pay-detail .pay-total {
  display: flex;
  flex-direction: column;
  width: 100%;
  padding: 40rpx 30rpx;
  box-sizing: border-box;
}

.pay-detail .pay-total .total-deposite {
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  justify-content: space-between;
  width: 100%;
  font-size: 28rpx;
  color: #999;
  line-height: 1;
  margin-bottom: 30rpx;
}

.pay-detail .pay-total .total-deposite .rmb {
  font-size: 20rpx;
}

.pay-detail .pay-total .free-deposite {
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  justify-content: space-between;
  width: 100%;
  font-size: 28rpx;
  color: #333;
  line-height: 1;
  margin-bottom: 30rpx;
}

.pay-detail .pay-total .free-deposite .rmb {
  font-size: 20rpx;
}

.pay-detail .pay-total .freeze-deposite {
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  justify-content: space-between;
  width: 100%;
  font-size: 28rpx;
  color: #333;
  line-height: 1;
}

.pay-detail .pay-total .freeze-deposite .rmb {
  font-size: 20rpx;
}

.operate-btn {
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  width: 100%;
  margin-top: 60rpx;
  box-sizing: border-box;
}

.operate-btn .check-order, .operate-btn .back-to-origin {
  width: 44%;
  height: 88rpx;
  outline: 0;
  border: 0;
  border-radius: 44rpx;
  font-size: 36rpx;
  line-height: 88rpx;
  margin-left: 4%;
}

.operate-btn .check-order {
  background-color: transparent;
  color: #FF584F;
  border: 1px solid #FF584F;
}

.operate-btn .check-order:active {
  /* background-color: rgba(0,0,0,.1); */
  opacity: .85;
}

.operate-btn .back-to-origin {
  background-color: #FF584F;
  color: #fff;
}

.operate-btn .back-to-origin:active {
  background-color: #FF584F;
}

.operate-btn .retry {
  width: 92%;
  height: 88rpx;
  background: linear-gradient(90deg, rgba(255, 129, 85, 1) 0%, rgba(255, 72, 77, 1) 100%);
  outline: 0;
  border: 0;
  border-radius: 44rpx;
  font-size: 36rpx;
  color: #fff;
  margin-left: 4%;
}

.operate-btn .retry:active {
  background-color: #03a68c;
}

.bottom {
  display: flex;
  justify-content: center;
  width: 100%;
  margin-top: 50rpx;
  padding-bottom: 55rpx;
}

.bottom .zhima {
  width: 526rpx;
  height: 95rpx;
}

.maiduan {
  color: rgb(153, 153, 153);
}

/* 下单成功 */

.order_status {
  text-align: center;
}

.order_status .status_title {
  font-size: 40rpx;
  font-family: PingFang SC;
  font-weight: 600;
  color: #333333;
  height: 42rpx;
  line-height: 42rpx;
  display: flex;
  align-items: center;
  align-content: center;
  justify-content: center;
  margin-top: 42rpx;
}

.order_status .status_title image {
  height: 42rpx;
  width: 42rpx;
  margin-right: 14rpx;
}

.order_status .mian {
  width: 191rpx;
  height: 23rpx;
  margin-top: 22rpx;
  margin-bottom: 50rpx;
}

.bottom_line {
  border-top: solid #DDDDDD 1rpx;
  margin-top: 41rpx;
  margin-bottom: 40rpx;
}

.order_infoItem {
  display: flex;
  justify-content: space-between;
  color: #FFFFFF;
  height: 26rpx;
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #999999;
  margin-bottom: 30rpx;
}

.order_infoItem1 {
  color: #333333;
}

.btns .success_btns .check-order {
  width: 330rpx;
  height: 88rpx;
  border: 1px solid #FF584F;
  border-radius: 44rpx;
  text-align: center;
  line-height: 88rpx;
  font-size: 36rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #FF584F;
}

.btns .success_btns .back {
  width: 330rpx;
  height: 88rpx;
  background: linear-gradient(90deg, rgba(255, 129, 85, 1) 0%, rgba(255, 72, 77, 1) 100%);
  border: 1px solid #FF584F;
  border-radius: 44rpx;
  text-align: center;
  line-height: 88rpx;
  font-size: 36rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: white;
}

.btns .success_btns {
  display: flex;
  justify-content: space-between;
  margin-top: -100rpx;
  padding: 0 20rpx;
}

/* 优惠券列表 */

.recomCoupon {
  display: flex;
  justify-content: space-between;
}

.recomCoupon_item {
  width: 216rpx;
  height: 217rpx;
  background: rgba(255, 88, 79, 0.07);
  border-radius: 10rpx;
