/* 详情 */

.goods-introduce {
  background-color: #fff;
  padding: 23rpx;
  margin: 65rpx 18rpx 20rpx 18rpx;
  box-sizing: border-box;
  border-radius: 20rpx 20rpx 20rpx 20rpx;
}


.goods-introduce .goods-name {
  font-size: 30rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: bold;
