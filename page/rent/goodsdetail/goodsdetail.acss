}

.card_coupon_r .coupon_details .coupon_title {
  font-size: 30rpx;
  font-family: PingFang SC-Bold, PingFang SC;
  font-weight: bold;
  color: #333333;
  margin-bottom: 8rpx;
}

.card_coupon_layout .card_coupon_r .claim_now{
  width: 148rpx;
  height: 51rpx;
  background: linear-gradient(180deg, #FF685D 0%, #FFA1A5 100%);
  border-radius: 26rpx 26rpx 26rpx 26rpx;
  font-size: 24rpx;
  font-family: PingFang SC-Bold, PingFang SC;
  font-weight: bold;
  color: #FFFFFF;
  text-align: center;
  line-height: 51rpx;
}
.card_coupon_layout .card_coupon_r .received{
  width: 148rpx;
  height: 51rpx;
  background: #D4D4D4;
  border-radius: 26rpx 26rpx 26rpx 26rpx;
  font-size: 24rpx;
  font-family: PingFang SC-Bold, PingFang SC;
  font-weight: bold;
  color: #FFFFFF;
  text-align: center;
  line-height: 51rpx;
}

