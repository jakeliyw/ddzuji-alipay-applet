        <text>国行正品保障</text>
      </view>
      <view class="anxin_renting_header_layout">
        <image mode="scaleToFill" src="/image/goodsdetail/sesame.png" class="anxin_renting_img"/>
        <text>芝麻信用合作</text>
        </view>
      <view class="anxin_renting_header_layout">
        <image mode="scaleToFill" src="/image/goodsdetail/empower.png" class="anxin_renting_img"/>
        <text>品牌官方授权</text>
      </view>
    </view>
    <view class="anxin_renting_layout">
      <view class="anxin_renting_query" onTap="jumpProduct">
        <text style="line-height: 41rpx">查看</text>
        <image mode="scaleToFill" src="/image/goodsdetail/query.png" class="query"/>
