      cate_pid="{{goodsDetail.cate_pid}}" currentIndex="{{goodsDetail.recommend_check==0?1:goodsDetail.recommend_check-1}}" />
    
  </view>
  <!-- 活动坑位 -->
  <view a:if="{{activity_banner!=''}}" class="activity-banner">
    <swiper autoplay="{{true}}" onChange="activitybannerIndex" interval="3000" circular="{{true}}">
      <block a:for="{{activity_banner}}">
        <swiper-item class="item">
          <view onTap="jump_tap" data-jump_type="{{item.jump_type}}" data-app_id="{{item.app_id}}" data-content_url="{{item.content_url}}" 
            data-telmp_id="{{item.telmp_id}}">
            <image lazy-load="{{true}}" src="{{(item.img_uri!=''&&item.img_uri)?imgUrl+item.img_uri:''}}" mode="scaleToFill" />
          </view>
        </swiper-item>
      </block>
    </swiper>
    <view class="swiper_dots">
      <view a:for="{{activity_banner}}" key="banner{{index}}" a:if="{{activity_banner.length>1}}" class="swiper_dots_item {{index==customBannerdotsindex?'active':''}}"></view>
    </view>
  </view>
  <!-- 规格参数 -->
  <view class="specifications">
    <view class="service-des por" data-origin="express" onTap="showPanel">
