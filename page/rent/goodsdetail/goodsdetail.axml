<view a:if="{{goodsDetail&&!noData}}">
  <!-- 收藏组件 -->
  <favorite a:if="{{showWhat.is_show_favorite=='yes'&&canUse_favorite}}">收藏小程序，下次使用更便捷</favorite>
  <view style="width:100%;background:#fff">
    <lifestyle public-id="2021002130605084" />
  </view>
  <!-- 轮播 -->
  <view class="swiper-top" style="position:relative">
    <image lazy-load="{{true}}" onTap="tapImage" mode="scaleToFill" src="../../../image/fenxiang.png" style="width:50rpx;height:50rpx;position:absolute;right:40rpx;z-index:9" />
    <swiper class="banner" onChange="bannerIndex" indicator-dots="{{false}}" indicator-color="rgb(119,119,119)" indicator-active-color="rgb(255,255,255)" 
      interval="3000" autoplay="{{true}}" circular="{{true}}">
      <block a:for="{{banner}}">
        <swiper-item class="item">
          <image lazy-load="{{true}}" src="{{(item!=''&& item)?imgUrl + item:''}}" mode="aspectFit"></image>
        </swiper-item>
      </block>
    </swiper>
    <view class="swiper-top-label" onTap="tapShow">
      <image class="fineness" mode="scaleToFill" src="/image/fineness.png"  />
      <text>成色</text>
    </view>
    <!-- <label a:if="{{banner.length>1}}" class="swiper-top-label">
      <text>{{current}}</text>
      /{{banner.length}}
    </label> -->
    <view class="zhima-pic">
      <view class="sesame_avoid_money">
        <image lazy-load="{{true}}" mode="scaleToFill" src="/image/shuidi.png" style="width:40rpx;height:40rpx;margin-left:20rpx;margin-right:20rpx" />
        <view class="v1" >
          芝麻免押金:550分以上即有机会享受押金全免
        </view>
      </view>
      <view class="swiper_page_current" a:if="{{banner.length>1}}">
        <text class="swiper_page_current_page">{{current}}/{{banner.length}}</text>
      </view>
    </view>
  </view>
  <view class="billions_subsidy">
    <image  mode="scaleToFill" src="/image/goodsdetail/subsidy_bg.png" class="subsidy_bg"/>
    <view class="subsidy_content">
      <view class="subsidy_price">
        <view class="price_layout">
          <view class="text_style">
            <image mode="scaleToFill" src="/image/goodsdetail/exclusive_price.png" a:if="{{ is_show_exclusive }}" class="exclusive_price_img"/>
            <text  class="delivery_price" a:else>到手价￥</text>
            <text class="delivery_number">{{goodsDetail.is_discount === 1 ? goodsDetail.after_discount_rent: goodsDetail.min_price}}</text>
            /天
            <view class="total_subsidy">
              <image  mode="scaleToFill" src="/image/goodsdetail/total_subsidy.png" class="total_subsidy_img"/>
              <view class="total_subsidy_money">共补贴￥1930</view>
            </view>
          </view>
          <view class="daily_price">
            日常价
            <text class="delete-line">￥{{goodsDetail.min_price}} /天</text>
            <image mode="scaleToFill" src="/image/goodsdetail/daily_icon.png" style="width: 20rpx; height: 20rpx"/>
          </view>
        </view>
      </view>
      <view class="billions_subsidy_link">
        <view class="billions_subsidy_layout">
          <image mode="scaleToFill" src="/image/goodsdetail/billions_subsidy.png" class="billions_subsidy_img"/>
          <image mode="scaleToFill" src="/image/goodsdetail/billions_subsidy_arrow.png" class="billions_subsidy_arrow"/>
        </view>
        <view class="free_shipping">
          租比买更省 | 包邮
        </view>
      </view>
    </view>
  </view>
