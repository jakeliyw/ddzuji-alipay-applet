}

/* 轮播label */

.swiper-top-label {
  background: rgba(174, 174, 174, 0.5988);
  border-radius: 20rpx;
  width: 94rpx;
  height: 38rpx;
  line-height: 38rpx;
  position: absolute;
  right: 30rpx;
  bottom: 50rpx;
  color: white;
  text-align: center;
  font-size: 24rpx;
  opacity: 0.6;
}

.swiper-top-label text {
  font-size: 32rpx;
}

.a-swiper-indicator .dot {
  border-radius: 50%;
}

/* 轮播点自定义 */

.swiper_dots {
  display: flex;
  position: absolute;
  right: 42rpx;
  margin-top: -24rpx;
  flex-direction: row;
}

.swiper_dots .active {
  background: white;
}

.swiper_dots_item {
  background: rgba(0, 0, 0, 0.5);
  width: 12rpx;
  height: 12rpx;
  margin-left: 6rpx;
  border-radius: 50%;
}

.fixed_activitie-view {
  background-color: white;
}

.fixed_activitie-view image {
  width: 100%;
  height: auto;
}

.item_fh image {
  width: 18rpx;
  height: 24rpx;
  margin-right: 4rpx;
}

.item_fh {
  display: flex;
  align-items: center;
}

/* 详情 */

.goods-introduce {
  width: 100%;
  background-color: #fff;
  padding: 25rpx 30rpx 30rpx;
  box-sizing: border-box;
  margin-top: -20rpx;
  border-radius: 20rpx 20rpx 0rpx 0rpx;
  position: relative;
}

.buynow-main-infomation {
  display: flex;
  align-items: baseline;
  color: #FF584F;
  margin-bottom: 30rpx;
}

.buynow-main-infomation .price-before {
  font-size: 24rpx;
  font-weight: 400;
}

