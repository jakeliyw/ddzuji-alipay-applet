  border-radius: 10rpx;
  color: #fff;
  align-self: center;
}

.triangle {
  width: 0;
  height: 0;
  border: 10rpx solid transparent;
  border-right-color: rgba(255, 129, 85, 1);
  position: absolute;
  left: -19rpx;
  top: 50%;
  transform: translateY(-50%)
}

.buynow-sub-infomation {
  font-size: 30rpx;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
  line-height: 40rpx;
}

