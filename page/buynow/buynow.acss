/* 通告栏 */

.buynow-notice {
  color: #F86A0D;
  font-size: 28rpx;
  padding: 16rpx 21rpx 16rpx 30rpx;
  background:rgba(253,251,234,1);
}

.buynow-notice .am-notice-scroll-left,.buynow-notice .am-notice-scroll-right {
